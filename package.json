    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.2",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.13",
    "yaml-loader": "^0.8.1"
